{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "A scatterplot showing number of students at the highest ranked unis",
  "data": {"url": "https://github.com/assignment22/datavis2/blob/main/World%20University%20Rankings%202023.csv"},
  "mark": "point",
  "transform": [{
    "calculate": "'https://www.google.com/search?q=' + datum.Name", "as": "url"
  }],
  "encoding": {
    "x": {"field": "Name of University", "type": "nominal"},
    "y": {"field": "No of student", "type": "quantitative"},
    "color": {"field": "Origin", "type": "nominal"},
    "tooltip": {"field": "Name OF University", "type": "nominal"},
    "href": {"field": "url", "type": "nominal"}
  }
}


